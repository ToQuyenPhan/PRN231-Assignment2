@page
@model ProjectParticipantManagementSystemWebClient.Pages.AdminPages.CompanyProjects.DeleteModel
@{
    ViewData["Title"] = "Delete Company Project";
}
<h1>Delete</h1>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Company Project</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Project Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyProject.ProjectName)
        </dd>
        <dt class="col-sm-2">
            Project Description
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyProject.ProjectDescription)
        </dd>
        <dt class="col-sm-2">
            Estimated Start Date
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyProject.EstimatedStartDate)
        </dd>
        <dt class="col-sm-2">
            Expected End Date
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyProject.ExpectedEndDate)
        </dd>
    </dl>   
    <form method="post">
        <input type="hidden" asp-for="CompanyProject.CompanyProjectID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./Index">Back to List</a>
    </form>
</div>
